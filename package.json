{
  "name": "l-exe-cf",
  "version": "1.0.0",
  "description": "LibreChat clone running on Cloudflare Workers",
  "type": "module",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "deploy:staging": "wrangler deploy --env staging",
    "deploy:production": "wrangler deploy --env production",
    "db:migrate": "wrangler d1 migrations apply l_exe_db",
    "db:migrate:local": "wrangler d1 migrations apply l_exe_db --local",
    "db:studio": "wrangler d1 studio l_exe_db",
    "typecheck": "tsc --noEmit",
    "lint": "eslint workers/src --ext .ts",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "cf-typegen": "wrangler types"
  },
  "dependencies": {
    "@hono/zod-validator": "^0.2.2",
    "better-auth": "^1.4.18",
    "date-fns": "^3.6.0",
    "hono": "^4.4.0",
    "jose": "^5.6.3",
    "js-tiktoken": "^1.0.21",
    "kysely-d1": "^0.4.0",
    "nanoid": "^5.0.7",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240620.0",
    "@types/node": "^20.14.2",
    "@typescript-eslint/eslint-plugin": "^7.13.0",
    "@typescript-eslint/parser": "^7.13.0",
    "@vitest/coverage-v8": "^1.6.0",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0",
    "wrangler": "^4.62.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "cloudflare-workers",
    "hono",
    "chat",
    "ai",
    "librechat"
  ],
  "author": "",
  "license": "MIT"
}
